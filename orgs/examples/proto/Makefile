# show the files that were generated by bazel and added to the source tree.
show-generated:
	bazel query 'kind("generated file", //orgs/examples/proto:*)'

show-proto-compile-targets:
	bazel query 'kind("proto_compile", //orgs/examples/proto/... except //orgs/examples/proto:assets)'

update:
	bazelisk run //:gazelle -- ./orgs/examples/proto
	bazelisk run //orgs/examples/proto:assets
	bazelisk run //:gazelle -- ./orgs/examples/proto
